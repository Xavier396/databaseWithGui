<BorderPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUI.DeliverControlPane">
   <top>
      <MenuBar BorderPane.alignment="CENTER">
        <menus>
          <Menu onAction="#goToTask" mnemonicParsing="false" text="快递任务">
            <items>
              <MenuItem mnemonicParsing="false" text="快递管理" />
            </items>
          </Menu>
          <Menu  mnemonicParsing="false" text="我的">
            <items>
              <MenuItem onAction="#goToInfoManager" mnemonicParsing="false" text="个人信息管理" />
            </items>
          </Menu>

        </menus>
      </MenuBar>
   </top>
</BorderPane>
